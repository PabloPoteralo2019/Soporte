@model IEnumerable<SOPORTE.Models.it>

@{
    ViewBag.Title = "";
}

<h2>IT</h2>

<div>
    @Html.ActionLink("Alta IT", "Create", new { area = "" }, new { @class = "btn btn-primary" })
    @Html.ActionLink("Volver", "UserDashBoard", "Home", new { area = "" }, new { @class = "btn btn-primary" })
</div>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.cuenta)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.apellido)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.activo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.rol)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.cuenta)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.apellido)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.activo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.rol)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id = item.it_id }) |
            @Html.ActionLink("Detalles", "Details", new { id = item.it_id }) |
            @Html.ActionLink("Borrar", "Delete", new { id = item.it_id })
        </td>
    </tr>
    }

   

</table>
